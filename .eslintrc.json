{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "new-cap": "off"
    },

    "extends": [
        "plugin:react/recommended",
        "xo",
        "prettier"
      ]
}
